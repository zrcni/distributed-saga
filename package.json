{
  "name": "distributed-saga",
  "version": "0.0.1-alpha.1",
  "author": "zrcni",
  "repository": "https://github.com/zrcni/distributed-saga.git",
  "license": "ISC",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/cjs/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/cjs/index.d.ts",
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js",
      "default": "./dist/cjs/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "prepublishOnly": "npm run clean:dist && npm run build && npm run clean:src",
    "clean:dist": "rm -rf ./dist",
    "clean:src": "rm -rf ./src && rm -rf ./tsconfig*.json && rm -rf jest.config.js && rm -rf .swcrc*",
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "swc src -d dist/cjs --config-file .swcrc --copy-files && tsc --project tsconfig.cjs.json",
    "build:esm": "swc src -d dist/esm --config-file .swcrc.esm --copy-files && tsc --project tsconfig.esm.json",
    "build:types": "tsc --project tsconfig.build.json",
    "build:tsc": "tsc && tsc-alias",
    "test": "jest --forceExit --detectOpenHandles",
    "lint:fix": "npm run lint -- --fix"
  },
  "dependencies": {
    "cancelable-promise": "^4.3.1",
    "dotenv": "^16.0.1",
    "ms": "^2.1.3",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.220",
    "@types/jest": "^28.1.6",
    "@types/ms": "^0.7.31",
    "@types/node": "^17.0.45",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "jest": "^28.1.1",
    "lint-staged": "^13.0.3",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.7.0",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.4",
    "wait-for-expect": "^3.0.2"
  }
}
